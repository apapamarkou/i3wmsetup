# Update Keyring for YoUNiX desktop environment

yprint --figlet -c "$DISTRO_NAME"
echo
yprint -s "Update Keyring"
echo

sudo pacman -S archlinux-keyring; 
sudo pacman-key --refresh-keys;

echo
read -p "Press Enter to continue..."